<sdf version='1.11'>
  <model name='planar_arm'>
    <link name='base_link'>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <collision name='base_link_collision'>
        <pose>0 0 0.25 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.5</length>
            <radius>0.10000000000000001</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name='base_link_visual'>
